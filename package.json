{
	"author": "T-Mobile Product & Technology Team",
	"description": "Extracts documentation from your project to produce a beautiful static website",
	"license": "Apache-2.0",
	"name": "@tmobile/docinator",
	"version": "0.4.3",
	"scripts": {
		"build": "tsc && cp -R src/hugo/site-template dist/hugo/site-template && npm link --force && npm prune && npm pack",
		"build:clean": "npm run clean && npm run build",
		"clean": "rm -f *.tgz && rm -rf dist && rm -rf dist/hugo/site-template",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"start": "ts-node src/cli.ts",
		"test": "mocha -r ts-node/register \\\"**/*.spec.ts\\\""
	},
	"dependencies": {
		"commander": "^6.1.0",
		"graceful-fs": "^4.2.4",
		"hugo-bin": "^0.69.0",
		"shelljs": "^0.8.4"
	},
	"devDependencies": {
		"@types/chai": "^4.2.13",
		"@types/chai-as-promised": "^7.1.3",
		"@types/mocha": "^8.0.3",
		"@types/node": "^14.11.5",
		"@types/shelljs": "^0.8.8",
		"@types/sinon": "^9.0.8",
		"@types/sinon-chai": "^3.2.5",
		"@typescript-eslint/eslint-plugin": "^4.4.0",
		"@typescript-eslint/parser": "^4.4.0",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"eslint": "^7.10.0",
		"eslint-config-prettier": "^6.12.0",
		"husky": "^4.3.8",
		"mocha": "^8.1.3",
		"npm-run-all": "^4.1.5",
		"prettier": "2.2.1",
		"sinon": "^9.2.0",
		"sinon-chai": "^3.5.0",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tmobile/docinator/"
	},
	"main": "dist/index.js",
	"preferGlobal": true,
	"bin": {
		"docinator": "./dist/cli.js"
	},
	"publishConfig": {
		"@tmobile:registry": "https://gitlab.com/api/v4/projects/13999280/packages/npm/"
	},
	"keywords": [
		"documentation",
		"docs",
		"documentation-as-code",
		"static-website-generator"
	]
}
